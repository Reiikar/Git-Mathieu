@article{GZ14,
	Annote = { Notes de lecture JP: Le decodage d'un code matriciel sur F_q est equivalent au probleme MinRank 
	 qui est NP complet. La difficulte est ici que les auteurs regardent le cas des codes
	  F_q^m lineaires. Le probleme est que les matrices qui interviennent dans le probleme
	   MinRank associe sont structures. Il y a un joli argument de reduction probabiliste 
	    du probleme MinRank structure au decodage d'un code lineaire generique sur Fq.
	     La reduction est meilleure dans le cas de la recherche d'un mot de rang minimal
	      que dans le cas d'un decodage d'un mot donne de l'espace ce qui est etonnant (il suffit que m soit   
	       lineaire dans la longueur dans le premier cas alors qu'il est quadratique dans le deuxieme).
	        Je conjecturerai bien que l'on doit pouvoir ameliorer la dependance de m en la longueur dans 
		 le deuxieme cas.
		  NB : attention il y a une petite erreur dans la version d'arxiv v1 dans l'enonce et la preuve du theoreme 8
		   qui est corrigee dans la version que m'a donnee Philippe et que j'ai mise dans le repertoire /papiers.},
	Author = {Gaborit, Philippe and Z{\'{e}}mor, Gilles},
	Journal = {IEEE Trans. Information Theory},
	Pages = {7245--7252},
	Title = {On the hardness of the decoding and the minimum distance problems for rank codes},
	Volume = {62(12)},
	Year = {2016},
	Bdsk-Url-1 = {http://arxiv.org/abs/1404.3482}}

